/* Rofi configuration */

configuration {
    modi:                   "run,ssh,window";
    matching:               "fuzzy";
    font:                   "Hack 11";
    lines:                  7;
    separator-style:        "solid";
    hide-scrollbar:         true;
    kb-accept-entry:        "Return,KP_Enter";
    kb-remove-to-eol:       "";
}
@theme                  "theme"

